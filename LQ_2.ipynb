    "def ask_database(question, method=1, plot=False, save_plot=None):\n",
    "    \n",
    "    try:\n",
    "        print(f\"Processing question: {question}\")\n",
    "        \n",
    "        # Choose the method\n",
    "        if method == 1:\n",
    "            sql_chain = create_sql_chain_v1()\n",
    "            sql_query = sql_chain.invoke({\"question\": question})\n",
    "        # elif method == 2:\n",
